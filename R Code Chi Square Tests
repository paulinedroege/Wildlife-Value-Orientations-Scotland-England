table_typology <- table(ukdata_subset$uk_country, ukdata_subset$typology)
rownames(table_typology) <- c("England", "Scotland")
colnames(table_typology) <- c("Distanced", "Traditionalist", "Mutualist", "Pluralist")
print(table_typology)
percent_tbl <- round(prop.table(table_typology, margin = 1)* 100, 1)
print(percent_tbl)
chisq.test(table_typology)
options(digits=2)
rownames(percent_tbl) <- c("England", "Scotland")
colnames(percent_tbl) <- c("Distanced", "Traditionalist", "Mutualist", "Pluralist")
print(percent_tbl)
ukdata <- read_excel("United_Kingdom.xlsx")
ukdata$religion<-factor(ukdata$religion, levels=1:9)
ukdata$typology <- factor(ukdata$typology)
tbl_typology_religion <- table(ukdata$religion, ukdata$typology)
rownames(tbl_typology_religion) <- c("Does not belong to any domination", "Catholic", "Protestant", "Orthodox", "Jewish", "Muslim", "Hindu", "Buddhist", "Other")
colnames(tbl_typology_religion) <- c("Distanced", "Traditionalist", "Mutualist", "Pluralist")
tbl_typology_religion
chisq.test(tbl_typology_religion, simulate.p.value = TRUE, B = 10000)
ukdata$religion_collapsed <- as.character(ukdata$religion)
ukdata$religion_collapsed[ukdata$religion %in% c("Orthodox", "Muslim", "Hindu", "Buddhist", "Other")] <- "Other religions"
ukdata$religion_collapsed <- factor(ukdata$religion_collapsed)
tbl_collapsed <- table( ukdata$religion_collapsed,ukdata$typology)
chisq.test(tbl_collapsed, simulate.p.value = TRUE, B = 10000)
percent_tbl <- round(prop.table(tbl_collapsed, margin = 1) * 100, 0)
percent_tbl
rownames(percent_tbl) <- c("Does not belong to any domination", "Catholic", "Protestant", "Orthodox", "Jewish", "Muslim", "Hindu", "Buddhist", "Other")
colnames(percent_tbl) <- c("Distanced", "Traditionalist", "Mutualist", "Pluralist")
percent_tbl
chisq.test(tbl_typology_religion)
tbl_religionimp_typology <- table(ukdata_subset$religion_importance, ukdata_subset$typology)
print(tbl_religionimp_typology)
chisq.test(tbl_religionimp_typology)
percent_tbl <- round(prop.table(tbl_religionimp_typology, margin = 1) * 100, 1)
rownames(percent_tbl) <- c("Not at all important","Not very important","Somewhat important","Very important")
colnames(percent_tbl) <- c("Distanced", "Traditionalist", "Mutualist", "Pluralist")
percent_tbl
