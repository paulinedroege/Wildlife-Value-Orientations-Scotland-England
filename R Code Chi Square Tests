ukdata$indigenous <- factor(ukdata$indigenous)
tbl_indigenous_typology <- table(ukdata$indigenous,ukdata$typology)
print(tbl_indigenous_typology)
chisq.test(tbl_indigenous_typology)
chisq.test(tbl_indigenous_typology, simulate.p.value = TRUE, B = 10000)
percent_tbl <- round(prop.table(tbl_indigenous_typology, margin = 1) * 100, 0)
rownames(percent_tbl) <- c("I identify my entire ancestry as Indigenous", "I identify a significant part of my ancestry as Indigenous", "I identify only a small part of my ancestry as Indigenous", "I do not identify any of my ancestry as Indigenous")
colnames(percent_tbl) <- c("Distanced", "Traditionalist", "Mutualist", "Pluralist")
percent_tbl
